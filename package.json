		"sanitize-html": "^2.11.0",
		"svelte": "^4.2.10",
		"svelte-check": "^3.6.4",
		"svelte-intersection-observer": "^1.0.0",
		"swiper": "^11.0.6",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
