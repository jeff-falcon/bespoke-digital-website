		"@sanity/client": "^6.12.4",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/animejs": "^3.1.12",
		"@types/cookie": "^0.6.0",
		"@types/nodemailer": "^6.4.14",
		"@types/sanitize-html": "^2.11.0",
		"@typescript-eslint/eslint-plugin": "^7.0.1",
